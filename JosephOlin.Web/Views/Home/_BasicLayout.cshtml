@using System.Web.Optimization
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/bundles/bootstrapcss")
    @Styles.Render("~/bundles/fontawesomecss")
    @Styles.Render("~/bundles/carme")
    @Styles.Render("~/bundles/yesteryear")
    @Styles.Render("~/bundles/commoncss")

    <title>@ViewBag.Title</title>   
</head>

<body class="bg-primary content">
        @Html.Partial("NavBar")
        @Html.Partial("JumboNav")
        @RenderBody()
        @Html.Partial("Footer")

        @Scripts.Render("~/bundles/jqueryjs")
        @Scripts.Render("~/bundles/bootstrapjs")

        @Html.Partial("GoogleAnalytics")
</body>
</html>