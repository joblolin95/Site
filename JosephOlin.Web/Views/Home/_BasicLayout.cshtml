@using System.Web.Optimization
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/bundles/bootstrapcss")
    @Styles.Render("~/bundles/fontawesomecss")
    @Styles.Render("~/bundles/carme")
    @Styles.Render("~/bundles/yesteryear")

    <style>
        #content {
            font-family: 'Carme', sans-serif;
            font-size: 20px;
            padding-top:70px;
        }
        #linkStyle {
            color: white;
        }
    </style>

    <title>@ViewBag.Title</title>   

</head>

<body class="bg-primary" id="content" >
     
        @Html.Partial("NavBar")
        @Html.Partial("JumboNav")
        @RenderBody()
        @Html.Partial("Footer")
</body>
</html>
